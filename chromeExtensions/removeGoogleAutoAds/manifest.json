{
  "manifest_version": 3,
  "name": "Remove Google Auto Ads",
  "version": "1.0",
  "description": "Removes .google-auto-placed divs from pages",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
