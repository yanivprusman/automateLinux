[Unit]
Description=CAD Dev Server (Shed)
After=network.target daemon.service
Wants=daemon.service

[Service]
Type=simple
WorkingDirectory=/opt/automateLinux/extraApps/cad/web
ExecStart=/usr/bin/npm run dev -- -p 3001
Restart=on-failure
RestartSec=5s
User=root
Group=root
Environment=NODE_ENV=development
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
