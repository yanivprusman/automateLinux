{
  "name": "common",
  "displayName": "common",
  "description": "hello",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [{
        "id": "commonActivity",
        "title": "common",
        "icon":  "resources/icon.svg"
      }]
    },
    "views": {
      "commonActivity":[
        {
          "id": "commonView",
          "name": "Activiti launch",
          "icon":  "resources/icon.svg"
        }
      ]
    },
    "commands": [
        {
            "command": "common.helloWorld",
            "title": "Hello World"
        },
        {
            "command": "common.launchActiviti",
            "title": "Launch Activiti"
        },
        {
            "command": "common.executeSelectedActivitiItem",
            "title": "Execute Selected Activiti Item"
        },
        {
            "command": "common.executeNextActivitiItem",
            "title": "Execute Next Activiti Item"
        }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
