{
  "name": "git",
  "displayName": "git",
  "description": "git",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:activeFileView",
    "onCommand:git.checkoutFileFromCommit",
    "onCommand:git.checkoutSelectedCommit"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "git-sidebar",
          "title": "Git Extension",
          "icon": "resources/git-icon.svg"
        }
      ]
    },
    "views": {
      "git-sidebar": [
        {
          "id": "activeFileView",
          "name": "Active File Commits"
        }
      ]
    },
    "commands": [
      {
        "command": "git.showActiveFile",
        "title": "Show Active File"
      },
      {
        "command": "git.checkoutFileFromCommit",
        "title": "Checkout File from Commit"
      },
      {
        "command": "git.checkoutSelectedCommit",
        "title": "Checkout Selected Commit"
      },
      {
        "command": "git.checkoutAndShow",
        "title": "Checkout and Show File"
      },
      {
        "command": "git.navigateUp",
        "title": "Navigate Up"
      },
      {
        "command": "git.navigateDown",
        "title": "Navigate Down"
      }
    ],
    "keybindings": [
      {
        "command": "git.checkoutSelectedCommit",
        "key": "enter",
        "when": "focusedView == activeFileView && viewItem == commitItem"
      },
      {
        "command": "git.navigateUp",
        "key": "up",
        "when": "focusedView == activeFileView"
      },
      {
        "command": "git.navigateDown",
        "key": "down",
        "when": "focusedView == activeFileView"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "git.checkoutSelectedCommit",
          "when": "view == activeFileView && viewItem == commitItem",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}